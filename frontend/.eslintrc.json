{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "amd": true,
    "node": true
  },
  "globals": {
    "chai": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unexpected-multiline": "off"
  }
}